{
    "name": "laravel/octane",
    "description": "octane",
    "keywords": ["laravel", "octane", "roadrunner"],
    "license": "MIT",
    "support": {
        "issues": "https://github.com/laravel/octane/issues",
        "source": "https://github.com/laravel/octane"
    },
    "authors": [
        {
            "name": "Taylor Otwell",
            "email": "taylor@laravel.com"
        }
    ],
    "repositories": [
        {
            "type": "path",
            "url": "./../laravel-framework"
        }
    ],
    "require": {
        "php": "^8.0",
        "illuminate/contracts": "^8.0",
        "laminas/laminas-diactoros": "^2.5",
        "symfony/psr-http-message-bridge": "^2.0"
    },
    "require-dev": {
        "guzzlehttp/guzzle": "^7.2",
        "laravel/framework": "8.x-dev",
        "mockery/mockery": "^1.4",
        "nunomaduro/collision": "^5.3",
        "orchestra/testbench": "^6.16",
        "phpunit/phpunit": "^9.3",
        "spatie/laravel-ray": "^1.14",
        "spiral/roadrunner": "^1.8"
    },
    "autoload": {
        "psr-4": {
            "Laravel\\Octane\\": "src"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Laravel\\Octane\\Tests\\": "tests"
        }
    },
    "bin": [
        "bin/roadrunner-worker",
        "bin/swoole-server"
    ],
    "scripts": {
        "test": "vendor/bin/phpunit --colors=always",
        "test-coverage": "vendor/bin/phpunit --coverage-html coverage"
    },
    "config": {
        "sort-packages": true
    },
    "extra": {
        "laravel": {
            "providers": [
                "Laravel\\Octane\\OctaneServiceProvider"
            ],
            "aliases": {
                "Octane": "Laravel\\Octane\\OctaneFacade"
            }
        }
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}
